[
  {
    "policyName": "JB_Iot_Policy",
    "policyArn": "arn:aws:iot:us-west-2:756336011133:policy/JB_Iot_Policy",
    "policyDocument": {
      "Version": "2012-10-17",
      "Statement": [
        {
          "Effect": "Allow",
          "Action": [
            "iot:Connect",
            "iot:Receive",
            "iot:Publish",
            "iot:Subscribe"
          ],
          "Resource": "*"
        }
      ]
    }
  },
  {
    "policyName": "kalam-dev1-Policy",
    "policyArn": "arn:aws:iot:us-west-2:756336011133:policy/kalam-dev1-Policy",
    "policyDocument": {
      "Version": "2012-10-17",
      "Statement": [
        {
          "Effect": "Allow",
          "Action": [
            "iot:Publish",
            "iot:Receive"
          ],
          "Resource": [
            "arn:aws:iot:us-west-2:756336011133:topic/sdk/test/java",
            "arn:aws:iot:us-west-2:756336011133:topic/sdk/test/Python",
            "arn:aws:iot:us-west-2:756336011133:topic/topic_1",
            "arn:aws:iot:us-west-2:756336011133:topic/topic_2"
          ]
        },
        {
          "Effect": "Allow",
          "Action": [
            "iot:Subscribe"
          ],
          "Resource": [
            "arn:aws:iot:us-west-2:756336011133:topicfilter/sdk/test/java",
            "arn:aws:iot:us-west-2:756336011133:topicfilter/sdk/test/Python",
            "arn:aws:iot:us-west-2:756336011133:topicfilter/topic_1",
            "arn:aws:iot:us-west-2:756336011133:topicfilter/topic_2"
          ]
        },
        {
          "Effect": "Allow",
          "Action": [
            "iot:Connect"
          ],
          "Resource": [
            "arn:aws:iot:us-west-2:756336011133:client/sdk-java",
            "arn:aws:iot:us-west-2:756336011133:client/basicPubSub",
            "arn:aws:iot:us-west-2:756336011133:client/sdk-nodejs-*"
          ]
        }
      ]
    }
  },
  {
    "policyName": "kalam-dev2",
    "policyArn": "arn:aws:iot:us-west-2:756336011133:policy/kalam-dev2",
    "policyDocument": {
      "Version": "2012-10-17",
      "Statement": [
        {
          "Effect": "Allow",
          "Action": [
            "iot:Connect",
            "iot:Receive",
            "iot:Publish",
            "iot:Subscribe"
          ],
          "Resource": "*"
        }
      ]
    }
  }
]
